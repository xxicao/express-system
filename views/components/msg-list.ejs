<div class="page-header">
    <h3>
        留言列表
        <a class="btn btn-primary btn-sm pull-right" href="/admin/msg/clear" role="button"><span class="glyphicon glyphicon-trash" aria-hidden="true"></span> 清空留言</a>
    </h3>
</div>
<div class="table-responsive">
    <table class="table table-striped table-bordered">
        <thead>
            <tr>
                <th>ID</th>
                <th>昵称</th>
                <th>留言</th>
                <th>邮箱</th>
                <th>操作</th>
            </tr>
        </thead>
        <tbody>
            <% for(var i=0; i< listData.length; i++) { %>
                <tr>
                    <td><%= listData[i].ID %></td>
                    <td><%= listData[i].username %></td>
                    <td><%= listData[i].usermsg %></td>
                    <td><%= listData[i].useremail %></td>
                    <td>
                        <a href="/admin/msg/delete/<%= listData[i].ID %>">
                            <span class="glyphicon glyphicon-trash" aria-hidden="true"></span>
                        </a>
                    </td>
                </tr>
            <% } %> 
               
        </tbody>
    </table>
</div>
            